{
  "private": true,
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec \"ts-node src/index.ts\"",
    "build": "tsc && mkdir -p dist/public && rsync -a public/ dist/public/ || true",
    "start": "node dist/index.js",
    "check": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier -w \"src/**/*.{ts,tsx,js,json,md,yml,yaml}\"",
    "prepare": "husky install",
    "fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "py-lint": "ruff check .",
    "py-fix": "ruff check . --fix && black .",
    "test": "vitest run",
    "test:ci": "vitest run",
    "html:check": "html-validate \"public/**/*.html\" \"src/**/*.html\"",
    "check:ts": "tsc --noEmit",
    "check:js": "eslint .",
    "check:py:ruff": "python -m ruff check .",
    "check:py:black": "python -m black --check .",
    "check:html": "html-validate 'public/**/*.html' 'src/**/*.html' || echo 'No HTML files found; skipped'",
    "check:all": "npm run check:ts && npm run check:js && npm run check:py:ruff && npm run check:py:black && npm run check:html",
    "post": "node --env-file=.env --experimental-strip-types ./src/post.ts",
    "post:x": "npm run post -- --to=x",
    "post:ig": "npm run post -- --to=instagram",
    "setup:keys": "node --experimental-strip-types ./scripts/key-wizard.ts",
    "check:py": "python -m ruff check . && python -m black --check .",
    "fix:py": "python -m ruff check . --fix && python -m black .",
    "post:slack": "npm run post -- --to=slack",
    "post:x:tmpl": "node --env-file=.env --experimental-strip-types ./src/post.ts --to=x --dateprefix --hashtags=$DEFAULT_HASHTAGS",
    "post:ig:tmpl": "node --env-file=.env --experimental-strip-types ./src/post.ts --to=instagram --dateprefix --hashtags=$DEFAULT_HASHTAGS",
    "post:slack:tmpl": "node --env-file=.env --experimental-strip-types ./src/post.ts --to=slack --dateprefix --hashtags=$DEFAULT_HASHTAGS",
    "post:bsky": "npm run post -- --to=bluesky",
    "post:masto": "npm run post -- --to=mastodon",
    "post:tmpl:x": "node --env-file=.env --experimental-strip-types ./src/post.ts --to=x --dateprefix --hashtags=$DEFAULT_HASHTAGS",
    "post:tmpl:ig": "node --env-file=.env --experimental-strip-types ./src/post.ts --to=instagram --dateprefix --hashtags=$DEFAULT_HASHTAGS",
    "post:tmpl:bsky": "npm run post -- --to=bluesky --dateprefix --hashtags=$DEFAULT_HASHTAGS",
    "post:tmpl:masto": "npm run post -- --to=mastodon --dateprefix --hashtags=$DEFAULT_HASHTAGS",
    "post:tmpl:img": "node --env-file=.env --experimental-strip-types ./src/post.ts --dateprefix --hashtags=$DEFAULT_HASHTAGS --image=public/sample.jpg",
    "post:x:video": "npm run post -- --to=x --dateprefix --hashtags=$DEFAULT_HASHTAGS --video-name",
    "post:tmpl:x:video": "node --env-file=.env --experimental-strip-types ./src/post.ts --to=x --dateprefix --hashtags=$DEFAULT_HASHTAGS --video-name",
    "post:tmpl:ig:video": "node --env-file=.env --experimental-strip-types ./src/post.ts --to=instagram --dateprefix --hashtags=$DEFAULT_HASHTAGS --video-name",
    "post:tmpl:bsky:video": "npm run post -- --to=bluesky --dateprefix --hashtags=$DEFAULT_HASHTAGS --video-name",
    "post:tmpl:masto:video": "npm run post -- --to=mastodon --dateprefix --hashtags=$DEFAULT_HASHTAGS --video-name",
    "summary:week": "node --env-file=.env --experimental-strip-types ./src/weekly-summary.ts",
    "yt:auth": "node --env-file=.env --experimental-strip-types ./scripts/yt-get-token.ts",
    "yt:upload": "node --env-file=.env --experimental-strip-types ./src/youtube-upload.ts",
    "serve": "npm run build && pkill -f \"node dist/app.js\" 2>/dev/null || true && node dist/app.js"
  },
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.5.2",
