    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^13.24.0",
    "googleapis": "^159.0.0",
    "html-validate": "^10.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "nodemon": "^3.1.10",
    "playwright": "^1.41.2",
    "prettier": "^3.6.2",
