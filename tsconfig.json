{
  "compilerOptions": {
    "target": "ES2020",
    "module": "NodeNext",             // ← ここを ESNext から NodeNext に
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "noEmit": true
  },
  "exclude": [
    "admin-ui",
    "wanstage-admin",
    "src/pages/**"
  ]
}
